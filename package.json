{
  "name": "@sanity/document-internationalization",
  "version": "4.0.0",
  "description": "Create unique translations of a document based on its language, joined by a shared reference document.",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/document-internationalization#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/document-internationalization/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/document-internationalization.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/icons": "^3.7.4",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/mutator": "^3.99.0",
    "@sanity/ui": "^3.1.8",
    "@sanity/uuid": "^3.0.2",
    "sanity-plugin-internationalized-array": "^3.1.5",
    "sanity-plugin-utils": "^1.6.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@sanity/pkg-utils": "6.13.5",
    "@sanity/plugin-kit": "^3.1.12",
    "@sanity/semantic-release-preset": "^4.1.8",
    "@types/react": "^18.3.24",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.2",
    "npm-run-all2": "^5.0.2",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "rimraf": "^4.4.1",
    "sanity": "^3.99.0",
    "semantic-release": "^23.1.1",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "sanity": "^3.40.0 || ^4.0.0-0",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
