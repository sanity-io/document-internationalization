{
  "name": "@sanity/document-internationalization",
  "version": "1.1.1",
  "description": "Provides document level translations for Sanity Studio",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "sanity-plugin",
    "i18n",
    "localization"
  ],
  "homepage": "https://github.com/sanity-io/document-internationalization#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/document-internationalization/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/document-internationalization.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "exports": {
    ".": {
      "types": "./lib/src/index.d.ts",
      "source": "./src/index.ts",
      "import": "./lib/index.esm.js",
      "require": "./lib/index.js",
      "default": "./lib/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "source": "./src/index.ts",
  "types": "./lib/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "scripts": {
    "prebuild": "npm run clean && plugin-kit verify-package --silent && pkg-utils",
    "build": "pkg-utils build --strict",
    "clean": "rimraf lib",
    "compile": "tsc --noEmit",
    "eslint": "eslint --ext=.js,.jsx,.mjs,.ts,.tsx --quiet",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --quiet --fix",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "dependencies": {
    "@cprecioso/country-flag-emoji": "^1.0.0",
    "@sanity/color": "^2.1.20",
    "@sanity/icons": "^2.0.0",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.0.0",
    "@sanity/uuid": "^3.0.1",
    "html-react-parser": "^1.4.12",
    "just-omit": "^2.0.1",
    "just-safe-get": "^4.0.1",
    "just-split": "^3.0.1",
    "pinst": "^2.1.6",
    "react-fast-compare": "^3.2.0",
    "react-twemoji": "^0.5.0",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@sanity/pkg-utils": "^1.17.2",
    "@sanity/plugin-kit": "^2.1.5",
    "@sanity/semantic-release-preset": "^2.0.2",
    "@types/lodash": "^4.14.179",
    "@types/react": "^18",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-router": "^5.1.0",
    "react-router-dom": "^5.1.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "sanity": "^3.8.1",
    "styled-components": "^5.3.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "lodash": "^4.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "rxjs": "^7.0.0",
    "sanity": "^3.0.0",
    "styled-components": "^5.2 || ^6"
  },
  "engines": {
    "node": ">=14"
  }
}
